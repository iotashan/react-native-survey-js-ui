{
  "name": "react-native-survey-js-ui-example",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config e2e/jest.config.js",
    "test:e2e-mcp": "jest --config e2e-mobile-mcp/jest.config.js",
    "test:e2e-mcp:basic": "jest --config e2e-mobile-mcp/jest.config.js --testNamePattern=\"Survey Flow\"",
    "test:e2e-mcp:cross-platform": "jest --config e2e-mobile-mcp/jest.config.js --testNamePattern=\"Cross-Platform\"",
    "test:e2e-mcp:errors": "jest --config e2e-mobile-mcp/jest.config.js --testNamePattern=\"Error Handling\"",
    "test:e2e-mcp:integration": "jest --config e2e-mobile-mcp/jest.config.js --testNamePattern=\"Integration\"",
    "test:all": "yarn test && yarn test:e2e-mcp"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.1.1",
    "@react-navigation/native": "^7.0.17",
    "@react-navigation/stack": "^7.3.3",
    "expo": "~52.0.0",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-survey-js-ui": "link:..",
    "survey-core": "file:../survey-core-rn"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/react": "~18.3.12",
    "@types/react-navigation": "^3.4.0",
    "flow-parser": "^0.273.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "react-native-builder-bob": "^0.40.11",
    "react-native-monorepo-config": "^0.1.9",
    "react-test-renderer": "18.3.1"
  },
  "private": true
}
